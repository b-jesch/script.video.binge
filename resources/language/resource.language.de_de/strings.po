msgctxt "#32000"
msgid "Binge Organizer"
msgstr "Binge Organizer"

msgctxt "#32001"
msgid "already played"
msgstr "bereits abgespielt"

msgctxt "#32002"
msgid "not played yet"
msgstr "noch nicht abgespielt"

msgctxt "#32003"
msgid "Playing Title %s of %"
msgstr "Spiele Titel %s von %s"

msgctxt "#32004"
msgid "There isn't a Title in the Binge List. Please add at least one Title into the Binge List using the Context Menu of Views within Movies, TV Shows or Videos."
msgstr "Es befindet sich kein Titel in der Bingeliste. Füge wenigstens einen Titel über das Kontextmenü in den Ansichten für Filme, Sreien oder Videos hinzu."

msgctxt "#32005"
msgid "All titles of the Bingelist have already been played. Change the state of the titles in the list or add new titles."
msgstr "Alle Titel der Bingelist wurden bereits abgespielt. Wechsle den Status der Titel in der Liste oder füge neue Titel hinzu."

# Context menues

msgctxt "#32010"
msgid "Add to Binge List"
msgstr "Zur Bingeliste hinzufügen"

msgctxt "#32011"
msgid "Play/Manage Binge List..."
msgstr "Bingeliste abspielen/verwalten..."

msgctxt "#32012"
msgid "Play all"
msgstr "Bingeliste abspielen"

msgctxt "#32013"
msgid "Play List from Entry above"
msgstr "ab Eintrag aufwärts abspielen"

msgctxt "#32014"
msgid "Toggle Play Status of Entry"
msgstr "Abspielstatus umschalten"

msgctxt "#32015"
msgid "Clear Binge List"
msgstr "Bingeliste löschen"