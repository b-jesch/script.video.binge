msgstr "Bingeliste abspielen"

msgctxt "#32014"
msgid "Play List from Entry above"
msgstr "ab Eintrag aufwärts abspielen"

msgctxt "#32015"
msgid "Toggle Play Status of Entry"
msgstr "Abspielstatus umschalten"

msgctxt "#32016"
msgid "Remove already played items"
msgstr "Entferne bereits gespielte Einträge"

msgctxt "#32017"
msgid "Clear Binge list"
msgstr "Bingeliste löschen"

